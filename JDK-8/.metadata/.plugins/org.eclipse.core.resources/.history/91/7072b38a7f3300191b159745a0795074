package br.eti.webstuff.estudosjdk8.builder;

import java.util.ArrayList;
import java.util.List;

import br.eti.webstuff.estudosjdk8.entidade.Perfil;
import br.eti.webstuff.estudosjdk8.enumeration.Nivel;

public class PopulaLista {
	
	
	public static List<Perfil> populaListaTecnologias(){
		
		List<Perfil> perfis = new ArrayList<>();
		
		perfis.add(new Perfil( 1, "JDK 8", Nivel.PLENO, true, true, 5));
		perfis.add(new Perfil( 2, "JDK 8", Nivel.PLENO, false, true, 6));
		perfis.add(new Perfil( 3, "JDK 8", Nivel.PLENO, false, true, 6));
		perfis.add(new Perfil( 4, "JDK 8", Nivel.PLENO, true, true, 8));
		
		perfis.add(new Perfil( 5, "JDK 8", Nivel.SENIOR, true, true, 9));
		perfis.add(new Perfil( 6, "JDK 8", Nivel.SENIOR, true, true, 10));
		perfis.add(new Perfil( 7, "JDK 8", Nivel.SENIOR, true, true, 11));
		perfis.add(new Perfil( 8, "JDK 8", Nivel.SENIOR, true, true, 12));
		
		
		perfis.add(new Perfil( 9, "Spring Boot", Nivel.SENIOR, true, true, 5 ));
		perfis.add(new Perfil( 10, "Rest Full", Nivel.PLENO, true, true, 36 ));
		perfis.add(new Perfil( 11, "Spring Data JPA", Nivel.PLENO, true, true, 5 ));
		perfis.add(new Perfil( 12, "PL/ SQL", Nivel.JUNIOR, true, true, 8 ));
		perfis.add(new Perfil( 13, "Protocolo HTTP", Nivel.SENIOR, true, true, 36 ));
		
		return perfis;
	}

}
