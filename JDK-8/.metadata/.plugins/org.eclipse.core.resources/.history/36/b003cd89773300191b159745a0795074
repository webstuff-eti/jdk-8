package br.eti.webstuff.estudosjdk8.builder;

import java.util.ArrayList;
import java.util.List;

import br.eti.webstuff.estudosjdk8.entidade.Tecnologia;

public class PopulaLista {
	
	
	public static List<Tecnologia> populaListaTecnologias(){
		
		List<Tecnologia> tecnologias = new ArrayList<>();
		
		tecnologias.add(new Tecnologia( "Spring Boot", 10, "Senior" ));
		tecnologias.add(new Tecnologia( "Spring Data JPA", 10, "Pleno" ));
		tecnologias.add(new Tecnologia( "JDK 8", 10, "Pleno" ));
		tecnologias.add(new Tecnologia( "PL/ SQL", 10, "Junior" ));
		tecnologias.add(new Tecnologia( "REST FULL", 10, "Pleno" ));
		tecnologias.add(new Tecnologia( "JSON", 10, "Pleno" ));
		
		return tecnologias;
	}

}
