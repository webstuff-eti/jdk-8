package br.eti.webstuff.estudosjdk8.builder;

import java.util.ArrayList;
import java.util.List;

import br.eti.webstuff.estudosjdk8.entidade.Tecnologia;
import br.eti.webstuff.estudosjdk8.enumeration.Nivel;

public class PopulaLista {
	
	
	public static List<Tecnologia> populaListaTecnologias(){
		
		List<Tecnologia> tecnologias = new ArrayList<>();
		
		//Integer id, String nome, Nivel nivel, Boolean curso, Boolean participacaoProjeto, Integer mesesAtuando
		
		tecnologias.add(new Tecnologia( 1, "JDK 8", Nivel.PLENO, true, true, 5));
		tecnologias.add(new Tecnologia( 1, "Spring Boot", Nivel.SENIOR, true, true, 5 ));
		tecnologias.add(new Tecnologia( 1, "Rest Full", Nivel.PLENO, true, true, 36 ));
		tecnologias.add(new Tecnologia( 1, "Spring Data JPA", Nivel.PLENO, true, true, 5 ));
		tecnologias.add(new Tecnologia( 1, "PL/ SQL", Nivel.JUNIOR, true, true, 8 ));
		tecnologias.add(new Tecnologia( 1, "Protocolo HTTP", Nivel.SENIOR, true, true, 36 ));
		
		return tecnologias;
	}

}
