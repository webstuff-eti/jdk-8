package br.eti.webstuff.estudosjdk8.entidade;

import br.eti.webstuff.estudosjdk8.enumeration.Nivel;

public class Tecnologia {

	private Integer id;
	private Integer mesesAtuando;
	private String nome;
	private Nivel nivel;
	private Boolean curso;
	private Boolean participacaoProjeto;
	
	
	
	public Tecnologia(Integer id, String nome, Nivel nivel, Boolean curso, Boolean participacaoProjeto, Integer mesesAtuando){
		this.id = id;
		this.mesesAtuando = mesesAtuando;
		this.nome = nome;
		this.nivel= nivel;
		this.curso = curso;
		this.participacaoProjeto = participacaoProjeto;
	}
	

	
}
